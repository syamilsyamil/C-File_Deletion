using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace ConsoleApplication1
{
    class File_Delete_model
    {
        public string yyyy { get; set; }
        public string mm { get; set; }
        public string dd { get; set; }
        public string hh { get; set; }
        public string min { get; set; }
        public string ss { get; set; }
        public string ms { get; set; }
        public int day { get; set; }
        public string myMsg = "Successfully delete the file which is ";
        public string myAlt = "Deletion cannot takes place!";
        public string myDateAfter1 = "DateAfter1 ";
        public string myCreatedAt = "CreatedAt ";
        public string myDateTimeNow = "Date Now: ";
        public string compareBetween = "Compare between ";

        public void Initialize(string a, string b, string c, string d, string e, string f)
        {
            this.myMsg = a;
            this.myAlt = b;
            this.myDateAfter1 = c;
            this.myCreatedAt = d;
            this.myDateTimeNow = e;
            this.compareBetween = f;
        }
    }
}
